        public boolean verifyAirline(String airlineName){
        	Select we = new Select (driver.findElement(AIRLINE));
        	we.selectByVisibleText(airlineName);
        	if (airlineName != null ) {
				return true;
			} else {
				return false;
				
			} 
        	 }
        
